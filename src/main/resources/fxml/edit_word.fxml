         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button fx:id="edit_word" layoutX="486.0" layoutY="473.0" mnemonicParsing="false" onAction="#handleClickEdit" prefHeight="41.0" prefWidth="74.0" text="Edit" />
      <Label layoutX="170.0" layoutY="187.0" prefHeight="41.0" prefWidth="89.0" text="Spelling" textFill="#e8e5e5">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="spelling" layoutX="295.0" layoutY="187.0" prefHeight="41.0" prefWidth="265.0" promptText="/Example/">
